{{t "label_how_did_i_get_here"}}

{{cat-search
    query=query
    onBlur=(action "onQueryChange" bubbles=false)
}}

<table>
    <thead>
        <th>
            <td>{{t "label_name"}}</td>
            <td>{{t "label_paths"}}</td>
            <td>{{t "label_email"}}</td>
            <td>{{t "label_phone"}}</td>
        </th>
    </thead>
    <tbody>
        {{#each model as |member|}}
            <tr>
                <td>{{member.fullName}}</td>
                <td></td>
                <td>{{member.email}}</td>
                <td>{{member.phone}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
